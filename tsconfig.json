{
  "compilerOptions": {
    "declaration": true,
    "module": "commonjs",
    "target": "es2017",
    "sourceMap": true,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "strictNullChecks": true,
    "experimentalDecorators": true 
  },
  "exclude": [
    "node_modules"
  ]
}